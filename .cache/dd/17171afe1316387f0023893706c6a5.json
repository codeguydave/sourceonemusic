ce":"components/App.js","original":{"line":57,"column":27}},{"name":"tracks","generated":{"line":119,"column":27},"source":"components/App.js","original":{"line":57,"column":38}},{"generated":{"line":120,"column":0},"source":"components/App.js","original":{"line":57,"column":11}},{"generated":{"line":120,"column":8},"source":"components/App.js","original":{"line":49,"column":12}},{"generated":{"line":120,"column":9},"source":"components/App.js","original":{"line":48,"column":8}},{"generated":{"line":121,"column":0},"source":"components/App.js","original":{"line":60,"column":5}},{"name":"Component","generated":{"line":125,"column":2},"source":"components/App.js","original":{"line":7,"column":18}},{"name":"App","generated":{"line":127,"column":15},"source":"components/App.js","original":{"line":65,"column":15}}],"sources":{"components/App.js":"import React, { Component } from 'react';\nimport Artist from './Artist';\nimport Tracks from './Tracks';\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n\nclass App extends Component {\n    state = { artistQuery: '', artist: null,  tracks: null};\n\n    updateArtistQuery = () => {\n       \n        this.setState({ artistQuery: event.target.value});\n    }\n\n    searchArtist = () => {\n        \n\n        fetch(`${API_ADDRESS}/artist/${this.state.artistQuery}`)\n        .then(response => response.json())\n        .then(json => {\n            \n\n            if (json.artist.total > 0) {\n                const artist = json.artists.items[0];\n                \n                console.log('artist', artist)\n                this.setState({artist});\n                fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n                .then(response => response.json())\n                .then(json => this.setState( {tracks: json.tracks}))\n                .catch(error => alert(error.message));\n            }\n        })\n        .catch(error => alert(error.message));\n    }\n\n    handleKeyPress = event => {\n        if (event.key === 'Enter') {\n          this.searchArtist(); \n        }\n    }\n    \n   \n    render() {\n        console.log('this.state', this.state);\n         \n      \n        return (\n            <div>\n           <h2>Music Master</h2>\n           <input onChange={this.updateArtistQuery}\n                  onKeyPress={this.handleKeyPress} \n                  placeholder='Search for a Artist' \n                  />\n           <button onClick={this.searchArtist}>Search</button>\n           <Artist artist={this.state.artist} />\n           <Tracks tracks={this.state.tracks} />\n            </div>\n        ) ;  \n    }\n    \n}\n\n\nexport default App;\n"},"lineCount":null}},"hash":"32906350cd189aaca69ff542a7d6da6e","cacheData":{"env":{}}}